{
  "name": "dicoding-final-submission-bfawdr",
  "author": "Syahda Romansyah <syahdarom@gmail.com>",
  "description": "The final submission of the 'Belajar Fundamental Aplikasi Web Dengan React' (Learn the Fundamental of Web Applications with React) course on the Dicoding about creating a notes app with React.js",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:host": "vite --host",
    "dev:msw": "vite --mode \"development:msw\"",
    "dev:host:msw": "vite --host --mode \"development:msw\"",
    "build": "vite build",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "format": "prettier -w \"**/*.{js,jsx,ts,tsx,json,yaml,yml,css,md,mdx}\"",
    "format-c": "prettier -c \"**/*.{js,jsx,ts,tsx,json,yaml,yml,css,md,mdx}\"",
    "preview": "vite preview",
    "postinstall": "husky install"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint",
    "*.{js,jsx,ts,tsx,json,yaml,yml,css,md,mdx}": "prettier -w"
  },
  "dependencies": {
    "@uiw/react-md-editor": "^4.0.2",
    "classnames": "^2.5.0",
    "nanoid": "^5.0.4",
    "primereact": "^10.2.1",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.12.0",
    "react-markdown": "^9.0.1",
    "react-router-dom": "^6.21.1",
    "react-syntax-highlighter": "^15.5.0",
    "rehype-highlight": "^7.0.0",
    "rehype-sanitize": "^6.0.0",
    "remark-gfm": "^4.0.0",
    "validator": "^13.11.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "eslint-plugin-unused-imports": "^3.0.0",
    "husky": "^8.0.0",
    "lint-staged": "^15.2.0",
    "msw": "^2.0.11",
    "postcss": "^8.4.32",
    "prettier": "3.1.1",
    "prettier-plugin-tailwindcss": "^0.5.10",
    "tailwindcss": "^3.4.0",
    "vite": "^5.0.8"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
